<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.noteapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".NoteEditActivity"></activity>
        <activity android:name=".Edit1Activity" />
        <activity android:name=".Edit_Page3Activity" />
        <activity android:name=".Confirmation_messageActivity" />
        <activity android:name=".Forgot_passwordActivity" />
        <activity android:name=".singupActivity" />
        <activity android:name=".LoginActivity" />
        <activity
            android:name=".home_pages"
            android:label="@string/title_activity_home_pages"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>